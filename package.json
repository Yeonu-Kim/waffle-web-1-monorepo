{
  "name": "monorepo",
  "packageManager": "yarn@4.6.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "unused:check": "knip",
    "check-all": "yarn workspaces foreach -A run check && yarn unused:check"
  },
  "devDependencies": {
    "eslint": "9.12.0",
    "knip": "5.33.3",
    "typescript": "5.6.3"
  }
}
