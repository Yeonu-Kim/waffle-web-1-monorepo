{
  "workspaces": {
    ".": {
      "entry": [],
      "project": [".prettierrc", "package.json", ".yarnrc.yml"]
    },
    "apps/*": {
      "entry": ["src/main.tsx"],
      "project": ["**/*.{ts,tsx}"]
    },
    "packages/*": {
      "entry": ["{index,cli}.ts"],
      "project": ["**/*.ts"]
    }
  },
  "ignoreDependencies": ["eslint", "prettier", "react", "@types/react"],
  "ignoreBinaries": ["eslint", "prettier", "tsc", "vite", "knip", "build-pkg"]
}
