{
  "workspaces": {
    ".": {
      "entry": [],
      "project": [".prettierrc", "package.json", ".yarnrc.yml"]
    },
    "apps/*": {
      "entry": ["src/main.tsx"],
      "project": ["src/**/*.{ts,tsx}"]
    },
    "packages/*": {
      "entry": ["{index,cli}.ts"],
      "project": ["**/*.ts"]
    }
  }
}
